{
    "name": "tt-package-demo",
    "version": "0.0.1",
    "description": "A demo package for Total TypeScript",
    "license": "MIT",
    "keywords": [
        "demo",
        "typescript"
    ],
    "homepage": "https://github.com/nestorgm/tt-package-demo",
    "bugs": {
        "url": "https://github.com/nestorgm/tt-package-demo/issues"
    },
    "author": "Nestor Gomez <nestorgm@gmail.com>",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nestorgm/tt-package-demo.git"
    },
    "files": [
        "dist"
    ],
    "main": "dist/index.js",
    "type": "module",
    "devDependencies": {
        "@arethetypeswrong/cli": "^0.15.4",
        "prettier": "^3.3.3",
        "tsup": "^8.2.4",
        "typescript": "^5.5.4"
    },
    "scripts": {
        "tsc": "./node_modules/typescript/bin/tsc",
        "build": "tsup",
        "ci": "npm run build && npm run check-format && npm run check-exports && npt run lint",
        "format": "prettier --write .",
        "check-format": "prettier --check .",
        "check-exports": "attw --pack .",
	"lint": "tsc"
    },
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": "./dist/index.js",
            "default": "./dist/index.cjs"
        }
    }
}
